{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.+(js|css|png|jpg|jpeg|gif|ico|json|xml|svg|woff|woff2|ttf|eot)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
